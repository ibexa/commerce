{
    "name": "ibexa/commerce",
    "type": "metapackage",
    "license": "proprietary",
    "description": "A meta package for installing Ibexa DXP Commerce edition",
    "repositories": [
        {
            "type": "composer",
            "url": "https://updates.ibexa.co"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ibexa/experience": "4.5.2",
        "ibexa/cart": "4.5.2",
        "ibexa/checkout": "4.5.2",
        "ibexa/storefront": "4.5.2",
        "ibexa/order-management": "4.5.2",
        "ibexa/payment": "4.5.2",
        "ibexa/shipping": "4.5.2"
    },
    "require-dev": {
        "ibexa/ci-scripts": "^0.2@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.5.x-dev"
        }
    }
}
